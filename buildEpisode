
if [ ! -f data/$1-$3.mp4 ]; then
    mkdir -p data/$1
    mkdir -p data/words
    python cut.py $1 $3
    #exit 0
    if [ ! -f data/$1-$3.vtt ]; then
        echo "Translating $1-$2.vtt"
        python translate.py data/$1-$2.vtt $3 > data/$1-$3.vtt
    fi
    python dictionary.py $1 $3
    python sayLines.py $1 1 $2
    python sayWords.py $1 2 $3
    python sayLines.py $1 3 $3

    python combine.py $1
    ./video $1 $3
fi
